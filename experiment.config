[model]
batch_size = 64
model_type = dt
embed_dim = 128
n_layer = 3
n_head = 1
activation_fn = relu
dropout = 0.1
lr = 1e-4
wd = 1e-4
warmup = 10000
wandb = True

k = 5
num_eval_episodes = 25
max_iters = 100
num_steps_per_iter = 10000
device = cuda            

[env]
env_name = fixed-d4rl
dataset = RVO_2_agent_5000/d4rl.p

mode = normal
max_ep_len = 1500
scale = 1
res = 128
state_mean = 0
state_std = 1
print_logs = True

[checkpoint]
checkpoint = False
resume = False
#load_path = models/mixed_4/20231019-1641.pth


[test]
model_path = models/fixed-d4rl-RVO_3-20240219-1842.pt
env_targets = 0.5, 1.0
multi = False
num_agents = 3
eval_episodes = 500
policies = external,RVO,RVO
# len = num_agents, make sure no space between policies ie. RVO,RVO
seed = 0